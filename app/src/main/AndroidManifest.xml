<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.elbourn.android.catgame">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/catgame_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/catgame_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CatGame"
        tools:targetApi="31">
        <activity
            android:name="com.elbourn.android.catgame.DisclaimerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.elbourn.android.catgame.IntroActivity" android:exported="false"/>
        <activity android:name="com.elbourn.android.catgame.MainActivity" android:exported="false"/>
    </application>

</manifest>